# Production environment variables for Firebase Hosting
# Copy this file to .env.production and fill in your actual values

# API Endpoints
VITE_DOCUMENTS_API_URL=https://your-documents-api-url.com
VITE_GENERATE_API_URL=https://your-generate-api-url.com

# Firebase Configuration
# Get these values after registering a Web app in Firebase Console
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=resume-tailor-saas.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=resume-tailor-saas
VITE_FIREBASE_MESSAGING_SENDER_ID=766176426144
VITE_FIREBASE_APP_ID=your-app-id

# Firebase Emulator (set to false in production)
VITE_USE_FIREBASE_EMULATOR=false

